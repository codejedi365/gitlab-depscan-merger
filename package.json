{
  "name": "gitlab-depscan-merger",
  "version": "0.0.0-semantically-versioned",
  "description": "Merger for multiple GitLab dependency scanning reports",
  "keywords": [
    "gitlab",
    "dependency",
    "audit",
    "scan",
    "monorepo",
    "ci"
  ],
  "homepage": "https://github.com/codejedi365/gitlab-depscan-merger#readme",
  "bugs": {
      "url": "https://github.com/codejedi365/gitlab-depscan-merger/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codejedi365/gitlab-depscan-merger.git"
  },
  "license": "MIT",
  "author": "codejedi365 <codejedi365@gmail.com> (https://github.com/codejedi365)",
  "contributors": [],
  "main": "dist/cli.min.cjs",
  "types": "dist/cli.d.ts",
  "bin": {
    "depscan-merger": "dist/cli.min.cjs"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build-watch": "npm run --silent build -- --watch",
    "check": "run-p --silent check:*",
    "check:security:dependencies": "npm audit --audit-level=moderate",
    "check:stale": "npm outdated",
    "commit": "git commit",
    "commit-retry": "CZ_RETRY=true git commit",
    "format": "npm run --silent lint -- --fix",
    "format:file": "npm run --silent lint:file -- --fix",
    "lint": "npm run --silent lint:file -- . --",
    "lint:file": "eslint",
    "test": "run-s --silent build test:*",
    "test:unit": "jest --selectProjects UNIT",
    "test:system": "jest --selectProjects SYSTEM --coverage=false",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "is-ci && run-s test",
    "tdd-watch": "npm run test:unit -- --watch"
  },
  "devDependencies": {
    "@codejedi365/jest-serial-runner": "^2.0.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.1",
    "@gitlab-org/security-report-schemas": "^14.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "@types/yargs": "^15.0.13",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "commitizen": "^4.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-mdx": "^1.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^2.5.4",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jest-extended": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "remark-lint-alphabetize-lists": "^3.0.0",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-preset-lint-consistent": "^4.0.0",
    "remark-preset-lint-markdown-style-guide": "^4.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-preset-prettier": "^0.4.1",
    "semantic-release": "^19.0.2",
    "semantic-release-npm-deprecate-old-versions": "^1.3.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-shebang-plugin": "^1.1.4",
    "yargs": "^17.0.1"
  },
  "engines": {
    "node": ">=12"
  }
}
